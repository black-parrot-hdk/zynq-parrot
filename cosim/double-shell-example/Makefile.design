DEFINES += GP1_ENABLE=1
DEFINES += GP1_ADDR_WIDTH=5 GP1_DATA_WIDTH=32
DEFINES += ACLK_MHZ=55
DEFINES += RTCLK_MHZ=8

include $(DESIGN_DIR)/Makefile.hardware

